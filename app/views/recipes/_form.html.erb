<%= form_for @recipe, html: { multipart: true } do |f| %>
	
	<%= f.text_field :title, :autofocus => true %>
	<%= f.label :title %>
	<br>
	<%= f.text_field :veggie1 %>
	<%= f.label :vegetable %>
	<br>
	<%= f.text_field :veggie2 %>
	<%= f.label :vegetable %>
	<br>
	<%= f.text_field :fruit1 %>
	<%= f.label :fruit %>
	<br>
	<%= f.text_field :fruit2 %>
	<%= f.label :fruit %>
	<br>
	<%= f.file_field :image %>
	<%= f.label :image %>
<br>
    <%= f.submit %>

<% end %>

